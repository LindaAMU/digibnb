<div class="container calendar">
  <%= simple_form_for [@service, @booking] do |f| %>
    <%= f.input :start_date, placeholder: "fecha de inicio", html5: true %>
    <%= f.input :end_date, placeholder:  "fecha de fin", html5: true %>
    <% if user_signed_in? %>
      <%= f.button :submit, class:"btn btn-dark m-2" %>
    <% else %>
      <%= link_to "Create account", new_user_session_path, class:"btn btn-dark m-2" %>
    <% end %>
  <% end %>
</div>
